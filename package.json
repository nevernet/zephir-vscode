{
    "name": "zephir",
    "version": "0.3.12",
    "displayName": "Zephir for VSCode",
    "title": "Zephir for VSCode",
    "description": "Zephir Language support for VSCode",
    "author": "Zephir Team",
    "license": "MIT",
    "publisher": "zephir-lang",
    "icon": "images/zephir.png",
    "readmeFilename": "README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/zephir-lang/zephir-vscode.git"
      },
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "language",
        "zephir",
        "highlight",
        "syntax",
        "snippets"
    ],
    "homepage": "https://github.com/zephir-lang/zephir-vscode",
    "bugs": {
        "url": "https://github.com/zephir-lang/zephir-vscode/issues"
    },
    "galleryBanner": {
        "color": "#272729",
        "theme": "dark"
    },
    "contributes": {
        "languages": [
            {
                "id": "zephir",
                "aliases": [
                    "Zephir",
                    "zephir"
                ],
                "extensions": [
                    ".zep"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "zephir",
                "scopeName": "source.zephir",
                "path": "./syntaxes/zephir.tmLanguage.json"
            }
        ]
    }
}